    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         SubscriptBox["k", "1"], "!"}], " ", 
        RowBox[{
         SubscriptBox["k", "2"], "!"}], " ", 
        RowBox[{
         SubscriptBox["k", "3"], "!"}]}], "..."}], 
      RowBox[{
       SubscriptBox["k", "r"], "!"}]}], ")"}]]}], TraditionalForm]], "Input",
 CellChangeTimes->{{3.9349473707230263`*^9, 3.9349474039808435`*^9}, 
